<h1>Books</h1>

<table>
  <thead>
    <th>Title</th>
    <th>Body</th>
    <th></th>
  </thead>
  <tbody>
    <tr></tr>
  </tbody>
</table>

<h2>New book</h2>
<%= form_with model:@book,local:true do |f| %>

<h4>Title</h4>
<%= f.text_field :title %>

<h4>Body</h4>
<%= f.text_area :body %>

<p><%= f.submit 'Create Book' %></p>

<% end %>